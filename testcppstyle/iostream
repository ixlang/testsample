static StdOut cout = new StdOut();
static String endl = "\n";

class StdOut{

	StdOut operator << (String txt){
		_system_.consoleWrite(txt);
        return this;
    }
    
    StdOut operator << (int txt){
		_system_.consoleWrite("" + txt);
        return this;
    }
    
    StdOut operator << (long txt){
		_system_.consoleWrite("" + txt);
        return this;
    }
    
    StdOut operator << (double txt){
		_system_.consoleWrite("" + txt);
        return this;
    }
};

void sleep(int n){
	Thread.sleep(n);
}

void AllocConsole(){
	_system_.createConsole();
}