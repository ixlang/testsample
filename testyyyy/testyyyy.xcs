class Integer{

	int ok;
	Integer(){
		ok = 0;
	}
	Integer(int i){
		ok = i;
	}
	Integer  operator +( Integer  o){
		_system_.consoleWrite(String.format("%d + %d\n", ok, o.ok));
		return new Integer(ok + o.ok);
	}
	Integer  operator -( Integer  o){
		_system_.consoleWrite(String.format("%d - %d\n", ok, o.ok));
		return new Integer(ok - o.ok);
	}
	Integer  operator *( Integer  o){
		_system_.consoleWrite(String.format("%d * %d\n", ok, o.ok));
		return new Integer(ok * o.ok);
	}
	Integer  operator /( Integer  o){
		_system_.consoleWrite(String.format("%d / %d\n", ok, o.ok));
		return new Integer(ok / o.ok);
	}
};

